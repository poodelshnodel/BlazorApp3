<PageTitle>MultiTable</PageTitle>

<h1 class=>Multiplication Table</h1>

<table class="table">
    <thead>
        <tr>
            <th></th>
            @for (int i = 1; i < 11; i++)
            {
                    <th>@i</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 1; i < 11; i++)
        {
                <tr>
                    <td>@i</td>
                @for (int j = 1; j < 11; j++)
                {
                    int result = i * j;
                            <td>@result</td>
                }
                </tr>
        }
    </tbody>
</table>